{
	"name": "n8n-nodes-weread",
	"version": "0.1.12",
	"description": "n8n community node for WeRead (微信读书) - Access your bookshelf, notes, bookmarks and reading data",
	"license": "MIT",
	"homepage": "https://github.com/funcodingdev/n8n-nodes-weread",
	"keywords": [
		"n8n-community-node-package",
		"weread",
		"微信读书",
		"reading",
		"books",
		"notes",
		"bookmarks"
	],
	"author": {
		"name": "funcodingdev",
		"email": "funcodingdev@foxmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/funcodingdev/n8n-nodes-weread.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": false,
		"credentials": [
			"dist/credentials/WereadManualCookieApi.credentials.js",
			"dist/credentials/WereadCookieCloudApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/weread/Weread.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
